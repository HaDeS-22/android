package com.example.practicalexam;

public class Employee {
    private String ssn;
    private String name;
    private int age;
    private String department;

    public Employee(String ssn, String name, int age, String department) {
        this.ssn = ssn;
        this.name = name;
        this.age = age;
        this.department = department;
    }

    public String getSsn() {
        return ssn;
    }

    public String getName() {
        return name;
    }

    public int getAge() {
        return age;
    }

    public String getDepartment() {
        return department;
    }
}

